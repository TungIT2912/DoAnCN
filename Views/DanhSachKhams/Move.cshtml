@model WebQuanLyNhaKhoa.Data.DanhSachKham

<h4>DanhSachKham</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Move" method="post" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Idkham" />
            <div class="form-group">
                <label asp-for="IdbenhNhan" class="control-label"></label>
                <select asp-for="IdbenhNhan" class="form-control" asp-items="ViewBag.IdbenhNhan"></select>
                <span asp-validation-for="IdbenhNhan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MaNv" class="control-label"></label>
                <select asp-for="MaNv" class="form-control" asp-items="ViewBag.MaNv"></select>
                <span asp-validation-for="MaNv" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NgayKham" class="control-label"></label>
                <input asp-for="NgayKham" class="form-control" />
                <span asp-validation-for="NgayKham" class="text-danger"></span>
            </div>
            <div class="form-group">
                <button type="submit">Đưa vào điều trị</button>
            </div>
