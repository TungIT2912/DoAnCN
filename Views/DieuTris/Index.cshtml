@model IEnumerable<WebQuanLyNhaKhoa.Data.DieuTri>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SoLuong)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ThanhTien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IddichVuNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IddungCuNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdkhamNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SoLuong)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ThanhTien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IddichVuNavigation.IddichVu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IddungCuNavigation.IddungCu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdkhamNavigation.Idkham)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IddieuTri">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IddieuTri">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IddieuTri">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
