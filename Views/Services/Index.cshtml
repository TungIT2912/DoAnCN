    <!-- Services Section -->

<section class="services-section">
    <div class="auto-container">
        <div class="row">
            <span class="d-block text-center mb-4">(Áp dụng cho ngày @DateTime.Now.ToString("dd/MM/yy"))</span>

            @foreach (var service in Model)
            {
                                var randomIconClass = iconClasses[random.Next(iconClasses.Count)];
                <div class="service-block col-lg-4 col-md-6 col-sm-12">
                    <div class="inner-box">
                        <span class="icon @randomIconClass"></span> 
                        <h5>
                            <a href="#" data-bs-toggle="modal" data-bs-target="#serviceModal-@service.IddichVu">
                                @service.TenDichVu
                            </a>
                        </h5>
                        <div class="text">
                            Đơn vị tính: @service.DonViTinh<br />
                            Giá: @service.DonGia.ToString("N0") VNĐ
                        </div>
                    </div>
                </div>

                <!-- Modal for the service -->
                <div class="modal fade" id="serviceModal-@service.IddichVu" tabindex="-1" aria-labelledby="serviceModalLabel-@service.IddichVu" aria-hidden="true">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title" id="serviceModalLabel-@service.IddichVu">@service.TenDichVu</h5>
                                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                            </div>
                                <div class="modal-body">
                                    <p><strong>Đơn vị tính:</strong> @service.DonViTinh</p>
                                    <p><strong>Giá:</strong> @service.DonGia.ToString("N0") VNĐ</p>
                                    <p><strong>Mô tả:</strong> @service.Description</p>
                                </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>


    </div>

 <!-- Pagination -->
            <div class="pagination-container pagination-services">
                @Html.PagedListPager(
                    Model,
                    page => Url.Action("Index", new { page }),
                    new PagedListRenderOptions
                    {
                        @* DisplayPageCountAndCurrentLocation = true, *@
